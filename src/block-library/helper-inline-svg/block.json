{
  	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "custom-block/helper-inline-svg",
	"version": "1.0.0",
	"title": "Inline SVG",
	"category": "custom-custom-blocks",
	"description": "Inline svg block.",
	"supports": {
		"html": false
	},
	"textdomain": "custom",
	"editorScript": "file:../../../build/js/helper-inline-svg/helper-inline-svg.js",
	"editorStyle": "file:../../../build/css/helper-inline-svg.css",
	"attributes": {
		"inline_svg_upload": {
			"type": "object",
			"default": ""
		},
		"inline_svg_width": {
			"type": "string",
			"default": "100"
		},
		"inline_svg_height": {
			"type": "string",
			"default": "auto"
		},
		"inline_svg_extra_css": {
			"type": "string",
			"default": ""
		},
		"inline_svg_style": {
			"type": "string",
			"default": ""
		}
	}
}
